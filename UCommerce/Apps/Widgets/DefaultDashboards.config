<configuration>
	<components>
		<component
				id="StartPageDashboard"
				service="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation"
				type="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation">
			<parameters>
				<section>StartPage</section>
				<defaultWidgets>
					<array>
						<value>${OrdersCount}</value>
						<value>${TurnOverByCurrency}</value>
						<value>${OrdersPerDayWidget}</value>
					</array>
				</defaultWidgets>
			</parameters>
		</component>

		<component
		id="StoresStartPageDashboard"
		service="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation"
		type="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation">
			<parameters>
				<section>StoresStartPage</section>
				<defaultWidgets>
					<array>
						<value>${CatalogSearch}</value>
					</array>
				</defaultWidgets>
			</parameters>
		</component>

		<component
		id="OrdersStartPageDashboard"
		service="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation"
		type="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation">
			<parameters>
				<section>OrdersStartPage</section>
				<defaultWidgets>
					<array>
						<value>${OrderListWidget}</value>
					</array>
				</defaultWidgets>
			</parameters>
		</component>

		<component
				id="MarketingStartPageDashboard"
				service="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation"
				type="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation">
			<parameters>
				<section>MarketingStartPage</section>
				<defaultWidgets>
					<array>
						<value>${MarketingTimelineWidget}</value>
					</array>
				</defaultWidgets>
			</parameters>
		</component>

		<component
		id="AnalyticsStartPageDashboard"
		service="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation"
		type="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation">
			<parameters>
				<section>AnalyticsStartPage</section>
				<defaultWidgets>
					<array>
					</array>
				</defaultWidgets>
			</parameters>
		</component>

		<component
		id="SettingsStartPageDashboard"
		service="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation"
		type="UCommerce.Presentation.UI.Dashboard, UCommerce.Presentation">
			<parameters>
				<section>SettingsStartPage</section>
				<defaultWidgets>
					<array>
					</array>
				</defaultWidgets>
			</parameters>
		</component>
	</components>
</configuration>
